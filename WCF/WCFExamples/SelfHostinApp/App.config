<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <behaviors>
            <serviceBehaviors>
                <behavior name="temp">
                    <serviceMetadata httpGetEnabled="true" />
                    <serviceDebug includeExceptionDetailInFaults="true" />
                </behavior>
            </serviceBehaviors>
        </behaviors>
        <services>
            <service name="SelfHostinApp.CustomerService" behaviorConfiguration="temp">
                <endpoint address="mex" binding="mexHttpBinding" contract="IMetadataExchange" />
                <endpoint binding="basicHttpBinding" bindingConfiguration=""
                    contract="SelfHostinApp.ICustomerService" />
              <endpoint address="tcp" binding="netTcpBinding" contract="SelfHostinApp.IInternalCustomerService"/>
              <endpoint address="mexTcp" binding="mexTcpBinding" contract="IMetadataExchange"/>
                <host>
                    <baseAddresses>
                        <add baseAddress="http://localhost:3333/MySelfHostingServices/" />
                    <add baseAddress="net.tcp://localhost:4444/MyTcpServices/" />
                    </baseAddresses>
                </host>
            </service>
        </services>
    </system.serviceModel>
</configuration>